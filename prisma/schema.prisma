generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model images {
  id        String   @id @unique
  url       String   @db.LongText
  prompt    String   @db.LongText
  height    Int
  width     Int
  size      Int
  channelId String
  createdAt DateTime
  storedAt  DateTime @default(now())
  updatedAt DateTime
}

model similar {
  id        Int      @id @default(autoincrement())
  prompt    String   @db.LongText
  imageId   Json
  createdAt DateTime @default(now())
  updatedAt DateTime
}
