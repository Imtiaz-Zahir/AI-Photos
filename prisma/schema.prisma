generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model photos {
  id             String   @id @map("_id")
  v              Int      @map("__v")
  build_At       DateTime @db.Date
  channelID      String
  createdAt      DateTime @db.Date
  displayImage   String
  download       Int
  height         Int
  prompt         String
  size           Int
  thumbnailImage String
  updatedAt      DateTime @db.Date
  url            String
  width          Int
}
