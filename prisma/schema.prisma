generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model images {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  v              Int      @map("__v")
  build_At       DateTime @db.Date
  channelID      String
  createdAt      DateTime @db.Date
  displayImage   String   @unique(map: "displayImage_1")
  download       Int
  height         Int
  id_            String   @unique(map: "id_1") @map("id")
  prompt         String
  size           Int
  thumbnailImage String   @unique(map: "thumbnailImage_1")
  updatedAt      DateTime @db.Date
  url            String   @unique(map: "url_1")
  width          Int
}
